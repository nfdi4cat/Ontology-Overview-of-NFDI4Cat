        <rdfs:range rdf:resource="https://schema.org/DateTime"/>
        <rdfs:comment xml:lang="en">The endTime of something. For a reserved event or service (e.g. FoodEstablishmentReservation), the time that it is expected to end. For actions that span a period of time, when the action was performed. e.g. John wrote a book from January to &lt;em&gt;December&lt;/em&gt;. For media, including audio and video, it&apos;s the time offset of the end of a clip within a larger file.&lt;br/&gt;&lt;br/&gt;

Note that Event uses startDate/endDate instead of startTime/endTime, even when describing dates with times. This situation may be clarified in future revisions.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/"/>
        <skos:editorialNote xml:lang="en">changed by m4i: added skos:prefLabel, added german label</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Endzeit</skos:prefLabel>
        <skos:prefLabel xml:lang="en">end time</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/startTime -->

    <owl:DatatypeProperty rdf:about="https://schema.org/startTime">
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#ProcessingStep"/>
        <rdfs:range rdf:resource="https://schema.org/DateTime"/>
        <rdfs:comment xml:lang="en">The startTime of something. For a reserved event or service (e.g. FoodEstablishmentReservation), the time that it is expected to start. For actions that span a period of time, when the action was performed. e.g. John wrote a book from &lt;em&gt;January&lt;/em&gt; to December. For media, including audio and video, it&apos;s the time offset of the start of a clip within a larger file.&lt;br/&gt;&lt;br/&gt;

Note that Event uses startDate/endDate instead of startTime/endTime, even when describing dates with times. This situation may be clarified in future revisions.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://schema.org/"/>
        <skos:editorialNote xml:lang="en">changed by m4i: added skos:prefLabel, added german label</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Startzeit</skos:prefLabel>
        <skos:prefLabel xml:lang="en">start time</skos:prefLabel>
    </owl:DatatypeProperty>
    


    <!-- https://schema.org/temporal -->

    <owl:DatatypeProperty rdf:about="https://schema.org/temporal">
        <rdfs:range rdf:resource="https://schema.org/DateTime"/>
        <skos:definition xml:lang="en">The &apos;temporal&apos; property can be used in cases where more specific properties (e.g., dateCreated, dateModified, datePublished) are not known to be appropriate</skos:definition>
        <skos:editorialNote xml:lang="en">changed by m4i: added different skos:prefLabel, added german label</skos:editorialNote>
        <skos:prefLabel xml:lang="en">has timestamp</skos:prefLabel>
        <skos:prefLabel xml:lang="de">hat Zeitstempel</skos:prefLabel>
    </owl:DatatypeProperty>
    


