    <!-- http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleUnitFor -->

    <owl:ObjectProperty rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#isAdmissibleUnitFor">
