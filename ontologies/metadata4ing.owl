        <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
        <skos:definition xml:lang="en">Declaration that can be applied to an si:Real quantity value to give an assessment of uncertainty</skos:definition>
        <skos:prefLabel xml:lang="de">Unsicherheitsdeklaration</skos:prefLabel>
        <skos:prefLabel xml:lang="en">uncertainty declaration</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#Assignment -->

    <owl:Class rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#Assignment">
        <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isValueInAssignment"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isVariableInAssignment"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#isAssignmentFor"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Assignment, i.e., a data item by which a value is assigned to a variable with respect to a particular referent (e.g., T = 200 K for substance o as it was measured in a particular cognitive step); therein, T is the variable, related to the assignment by m4i:hasVariable, 200 K is the value, related to the assignment by m4i:hasValue, and o is the referent, related to the assignment by isAssignmentFor</rdfs:comment>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Assignment</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Zuweisung</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#Property -->

    <owl:Class rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#Property">
        <rdfs:subClassOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Variable"/>
        <skos:broader rdf:resource="http://purl.obolibrary.org/obo/BFO_0000020"/>
        <skos:closeMatch rdf:resource="http://emmo.info/emmo#EMMO_b7bcff25_ffc3_474e_9ab5_01b1664bd4ba"/>
        <skos:definition xml:lang="en">Property, i.e., a variable that can be considered (as an investigated property, i.e., target property) within a processing step</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Eigenschaft</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Property</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue -->

    <owl:Class rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue">
        <rdfs:subClassOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#Value"/>
        <skos:definition xml:lang="en">Quantity value, i.e., a value that has a numerical magnitude and a physical unit</skos:definition>
        <skos:editorialNote>changed by m4i: added skos:prefLabel, adjusted german labels, harmonized spelling to OBO naming conventions (lower case, space separated)</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Größenwert</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Quantity value</skos:prefLabel>
    </owl:Class>
    


    <!-- http://www.molmod.info/semantics/pims-ii.ttl#Value -->

    <owl:Class rdf:about="http://www.molmod.info/semantics/pims-ii.ttl#Value">
        <rdfs:subClassOf rdf:resource="https://schema.org/Intangible"/>
