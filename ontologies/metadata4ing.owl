        <owl:disjointWith rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment>An organization.</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Organisation</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Organization</skos:prefLabel>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Person -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Person">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>A person.</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Person</skos:prefLabel>
        <skos:prefLabel xml:lang="en">Person</skos:prefLabel>
    </owl:Class>
    


    <!-- https://ptb.de/si/CoverageInterval -->

    <owl:Class rdf:about="https://ptb.de/si/CoverageInterval">
        <rdfs:subClassOf rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#UncertaintyDeclaration"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasCoverageProbability"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasIntervalMax"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasIntervalMin"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasStandardUnc"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasDistribution"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">Structure for stating a probabilistic-symmetric coverage interval for a real uncertainty, to be applied to an si:Real quantity value</skos:definition>
        <skos:editorialNote>changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="en">coverage interval</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Überdeckungsintervall</skos:prefLabel>
    </owl:Class>
    


    <!-- https://ptb.de/si/ExpandedUnc -->

    <owl:Class rdf:about="https://ptb.de/si/ExpandedUnc">
        <rdfs:subClassOf rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#UncertaintyDeclaration"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasCoverageFactor"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasCoverageProbability"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasUncertainty"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://ptb.de/si/hasDistribution"/>
                <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">Structure for stating an expanded measurement, model, or simulation uncertainty, e.g., to be applied to an si:Real quantity value</skos:definition>
        <skos:editorialNote>changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">erweiterte Unsicherheit</skos:prefLabel>
        <skos:prefLabel xml:lang="en">expanded uncertainty</skos:prefLabel>
    </owl:Class>
    


    <!-- https://ptb.de/si/Real -->

    <owl:Class rdf:about="https://ptb.de/si/Real">
        <rdfs:subClassOf rdf:resource="http://www.molmod.info/semantics/pims-ii.ttl#QuantityValue"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#hasUncertaintyDeclaration"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#hasNumericalValue"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:definition xml:lang="en">(Real value +- uncertainty) x measurement unit, i.e., a scalar value with a measurement unit and an uncertainty indication</skos:definition>
        <skos:editorialNote>changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Real</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Reell</skos:prefLabel>
    </owl:Class>
    


    <!-- https://schema.org/Intangible -->

    <owl:Class rdf:about="https://schema.org/Intangible">
        <skos:definition xml:lang="en">A utility class that serves as the umbrella for a number of &apos;intangible&apos; things such as quantities, structured values, etc.</skos:definition>
        <skos:editorialNote xml:lang="en">changed by m4i: added skos:prefLabel, added german label</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Intangible</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Ungreifbares</skos:prefLabel>
    </owl:Class>
    


    <!-- https://schema.org/Project -->

    <owl:Class rdf:about="https://schema.org/Project">
        <rdfs:comment xml:lang="en">An enterprise (potentially individual but typically collaborative), planned to achieve a particular aim</rdfs:comment>
        <rdfs:comment xml:lang="de">Ein typischerweise kollektives Unterfangen zum Erreichen eines vorab geplanten Ziels</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added German definition, added language tag to English definition, shortened definition</skos:editorialNote>
        <skos:editorialNote xml:lang="en">changed by m4i: use of skos:prefLabel instead of rdfs:label, added German label, added language tag to English label</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Project</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Projekt</skos:prefLabel>
    </owl:Class>
    


    <!-- https://schema.org/ResearchProject -->

    <owl:Class rdf:about="https://schema.org/ResearchProject">
        <rdfs:subClassOf rdf:resource="https://schema.org/Project"/>
        <rdfs:comment xml:lang="en">A research project</rdfs:comment>
        <rdfs:comment xml:lang="de">ein Forschungsprojekt</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added German definition, added language tag to English definition</skos:editorialNote>
        <skos:editorialNote xml:lang="en">changed by m4i: changed label to &quot;research project&quot; (no CamelCase)</skos:editorialNote>
        <skos:editorialNote xml:lang="en">changed by m4i: use of skos:prefLabel instead of rdfs:label, added German label, added language tag to English label</skos:editorialNote>
        <skos:prefLabel xml:lang="de">Forschungsprojekt</skos:prefLabel>
        <skos:prefLabel xml:lang="en">research project</skos:prefLabel>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#ContactPerson -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#ContactPerson">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person with knowledge of how to access, troubleshoot, or otherwise field issues related to the resource</skos:definition>
        <skos:prefLabel xml:lang="de">Kontaktperson</skos:prefLabel>
        <skos:prefLabel xml:lang="en">contact person</skos:prefLabel>
        <skos:scopeNote xml:lang="en">May also be the &quot;Point of Contact&quot; in an organisation that controls access to the resource, if that organisation is different from the Publisher, Distributor, and Data Manager.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#DataCollector -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#DataCollector">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person/institution responsible for finding or gathering/collecting data under the guidelines of the author(s) or Principal Investigator (PI)</skos:definition>
        <skos:prefLabel xml:lang="de">Datenerfasser*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">data collector</skos:prefLabel>
        <skos:scopeNote xml:lang="en">May also be used when crediting survey conductors, interviewers, event or condition observers, or persons responsible for monitoring key instrument data.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#DataCurator -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#DataCurator">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person tasked with reviewing, enhancing, cleaning, or standardizing metadata and the associated data submitted for storage, use, and maintenance within a data centre or repository</skos:definition>
        <skos:prefLabel xml:lang="de">Datenkurator*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">data curator</skos:prefLabel>
        <skos:scopeNote xml:lang="en">While the DataManager is concerned with digital maintenance, the DataCurator&apos;s role encompasses quality assurance focused on content and metadata. DataCurator responsibilities include: checking completeness of the submitted dataset against the content as described by the submitter; verifying standard metadata according to the applicable system or schema; adding or verifying specialized metadata to add value and ensure access across disciplines; and determining how the metadata might map to search engines, database products, and automated feeds. Repository managers as well as data librarians working in the repository fall within this category. 

Example: https://doi.org/10.20375/0000-000D-1D1F-2</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#DataManager -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#DataManager">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person (or organisation with a staff of data managers, such as a data centre) responsible for maintaining the finished resource</skos:definition>
        <skos:prefLabel xml:lang="de">Datenverwalter*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">data manager</skos:prefLabel>
        <skos:scopeNote xml:lang="en">The work done by this person or organisation ensures that the resource is periodically &quot;refreshed&quot; in terms of software/hardware support, is kept available or is protected from unauthorized access, is stored in accordance with industry standards, and is handled in accordance with the records management requirements applicable to it.

Example: https://doi.org/10.6073/pasta/41b3ed2e152e1e4c3846e646118208e7</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#Distributor -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Distributor">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Institution tasked with responsibility to generate/disseminate copies of the resource in either electronic or print form</skos:definition>
        <skos:prefLabel xml:lang="de">Anbieter*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">distributor</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Works stored in more than one archive/repository may credit each as a distributor.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#Editor -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Editor">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">A person who oversees the details related to the publication format of the resource</skos:definition>
        <skos:prefLabel xml:lang="de">Herausgeber*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">editor</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Note: if the Editor is to be credited in place of multiple creators, the Editor&apos;s name may be supplied as Creator, with &quot;(Ed.)&quot; appended to the name.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#HostingInstitution -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#HostingInstitution">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Typically, the organisation allowing the resource to be available on the internet through the provision of its hardware/software/operating support</skos:definition>
        <skos:prefLabel xml:lang="de">bereitstellende Institution</skos:prefLabel>
        <skos:prefLabel xml:lang="en">hosting institution</skos:prefLabel>
        <skos:scopeNote xml:lang="en">This role normally falls on the University, research center or organization where the data center/data repository belongs.

EXAMPLE: Université Grenoble Alpes (UGA)

May also be used for an organisation that stores the data offline-often a data centre if that data centre is not the &quot;publisher&quot; of the resource.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#Other -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Other">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Any person or institution making a significant contribution to the development and/or maintenance of the resource, but whose contribution is notadequately described by any of the other values for contributorType</skos:definition>
        <skos:editorialNote>Label adjusted. Do we really need to define all these person roles in our ontology? (GIACOMO)</skos:editorialNote>
        <skos:prefLabel xml:lang="en">other person</skos:prefLabel>
        <skos:prefLabel xml:lang="de">weitere Person</skos:prefLabel>
        <skos:scopeNote xml:lang="en">Could be a photographer, artist, or writer whose contribution helped to publicize the resource (as opposed to creating it), a reviewer of the resource, someone providing administrative services to the author (such as depositing updates into an online repository, analysing usage, etc.), or one of many other roles.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#Producer -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Producer">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Typically,a person or organisation responsible for the artistry and form of a media product</skos:definition>
        <skos:prefLabel xml:lang="de">Produzent*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">producer</skos:prefLabel>
        <skos:scopeNote xml:lang="en">In the data industry, this may be a company &quot;producing&quot; DVDs that package data for future dissemination by a distributor.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#ProjectLeader -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#ProjectLeader">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person officially designated as head of project team or sub-project team instrumental in the work necessary to development of the resource</skos:definition>
        <skos:prefLabel xml:lang="de">Projektleiter*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">project leader</skos:prefLabel>
        <skos:scopeNote xml:lang="en">The Project Leader is not &quot;removed&quot; from the work that resulted in the resource; he or she remains intimately involved</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#ProjectManager -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#ProjectManager">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person officially designated as manager of a project. Project may consist of one or many project teams and sub-teams.</skos:definition>
        <skos:prefLabel xml:lang="de">Projektmanager*in</skos:prefLabel>
        <skos:prefLabel xml:lang="en">project manager</skos:prefLabel>
        <skos:scopeNote xml:lang="en">The manager of a project normally has more administrative responsibility than actual work involvement.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#ProjectMember -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#ProjectMember">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Person on the membership list of a designated project/project team</skos:definition>
        <skos:prefLabel xml:lang="de">Projektmitglied</skos:prefLabel>
        <skos:prefLabel xml:lang="en">project member</skos:prefLabel>
        <skos:scopeNote xml:lang="en">This vocabulary may or may not indicate the quality, quantity, or substance of the person&apos;s involvement.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#RegistrationAgency -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#RegistrationAgency">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Institution/organisation officially appointed by a Registration Authority to handle specific tasks within a defined area of responsibility</skos:definition>
        <skos:prefLabel xml:lang="de">Registrierungsstelle</skos:prefLabel>
        <skos:prefLabel xml:lang="en">registration agency</skos:prefLabel>
        <skos:scopeNote xml:lang="en">DataCite is a Registration Agency for the International DOI Foundation (IDF). One of DataCite’s tasks is to assign DOI prefixes to the allocating agents who then assign the full, specific character string to data clients, provide metadata back to the DataCite registry, etc.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#RegistrationAuthority -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#RegistrationAuthority">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">A standards-setting body from which Registration Agencies obtain official recognition and guidance</skos:definition>
        <skos:prefLabel xml:lang="de">Registrierungsbehörde</skos:prefLabel>
        <skos:prefLabel xml:lang="en">registration authority</skos:prefLabel>
        <skos:scopeNote xml:lang="en">The IDF serves as the Registration Authority for the International Standards Organisation (ISO) in the area/domain of Digital Object Identifiers.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#RelatedPerson -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#RelatedPerson">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">A person without a specifically defined role in the development of the resource, but who is someone the author wishes to recognize</skos:definition>
        <skos:prefLabel xml:lang="en">related person</skos:prefLabel>
        <skos:prefLabel xml:lang="de">zugehörige Person</skos:prefLabel>
        <skos:scopeNote xml:lang="en">This person could be an author&apos;s intellectual mentor, a person providing intellectual leadership in the discipline or subject domain, etc.</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#ResearchGroup -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#ResearchGroup">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://schema.datacite.org/meta/kernel-4.4/doc/DataCite-MetadataKernel_v4.4.pdf?page=40</rdfs:seeAlso>
        <skos:definition xml:lang="en">Typically refers to a group of individuals with a lab, department, or divisionthathas a specifically defined focus of activity.</skos:definition>
        <skos:prefLabel xml:lang="de">Forschungsgruppe</skos:prefLabel>
        <skos:prefLabel xml:lang="en">research group</skos:prefLabel>
        <skos:scopeNote xml:lang="en">May operate at a narrower level of scope; may or may not hold less administrative responsibility than a project team.

Example: Space Research &amp; Planetary Sciences Division of the University of Bern (WP Unibe)

Source: https://doi.org/10.26302/SSHADE/EXPERIMENT_OP_20201104_001</skos:scopeNote>
    </owl:NamedIndividual>
    


    <!-- http://w3id.org/nfdi4ing/metadata4ing#Researcher -->

    <owl:NamedIndividual rdf:about="http://w3id.org/nfdi4ing/metadata4ing#Researcher">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Role"/>
