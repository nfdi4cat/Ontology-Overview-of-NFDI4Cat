        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#ProcessingStep"/>
        <rdfs:range rdf:resource="http://w3id.org/nfdi4ing/metadata4ing#ProcessingStep"/>
        <skos:altLabel xml:lang="en">finished by</skos:altLabel>
        <skos:definition xml:lang="en">x ends with y if and only if x has part y and the time point at which x ends is equivalent to the time point at which y ends. Formally: α(y) &gt; α(x) ∧ ω(y) = ω(x), where α is a function that maps a process to a start point, and ω is a function that maps a process to an end point.</skos:definition>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="de">endet mit</skos:prefLabel>
        <skos:prefLabel xml:lang="en">ends with</skos:prefLabel>
    </owl:ObjectProperty>
    


