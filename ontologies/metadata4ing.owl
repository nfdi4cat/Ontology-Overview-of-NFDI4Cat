    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Agent">
        <rdfs:comment>An agent (eg. person, group, software or physical artifact).</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Agent</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Akteur</skos:prefLabel>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Group -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Group">
        <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>A class of Agents.</rdfs:comment>
        <skos:editorialNote xml:lang="en">changed by m4i: added german labels, added skos:prefLabel</skos:editorialNote>
        <skos:prefLabel xml:lang="en">Group</skos:prefLabel>
        <skos:prefLabel xml:lang="de">Gruppe</skos:prefLabel>
    </owl:Class>
    


    <!-- http://xmlns.com/foaf/0.1/Organization -->

    <owl:Class rdf:about="http://xmlns.com/foaf/0.1/Organization">
