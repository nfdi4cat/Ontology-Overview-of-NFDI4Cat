schema:Project rdf:type owl:Class ;
               rdfs:comment "An enterprise (potentially individual but typically collaborative), planned to achieve a particular aim"@en ,
                            "Ein typischerweise kollektives Unterfangen zum Erreichen eines vorab geplanten Ziels"@de ;
               skos:editorialNote "changed by m4i: added German definition, added language tag to English definition, shortened definition"@en ,
                                  "changed by m4i: use of skos:prefLabel instead of rdfs:label, added German label, added language tag to English label"@en ;
               skos:prefLabel "Project"@en ,
