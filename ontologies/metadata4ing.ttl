foaf:Organization rdf:type owl:Class ;
                  rdfs:subClassOf foaf:Agent ;
                  owl:disjointWith foaf:Person ;
                  rdfs:comment "An organization." ;
                  skos:editorialNote "changed by m4i: added german labels, added skos:prefLabel"@en ;
                  skos:prefLabel "Organisation"@de ,
                                 "Organization"@en .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class ;
            rdfs:subClassOf foaf:Agent ;
            rdfs:comment "A person." ;
            skos:editorialNote "changed by m4i: added german labels, added skos:prefLabel"@en ;
            skos:prefLabel "Person"@de ,
                           "Person"@en .


###  https://ptb.de/si/CoverageInterval
si:CoverageInterval rdf:type owl:Class ;
                    rdfs:subClassOf m4i:UncertaintyDeclaration ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty si:hasCoverageProbability ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty si:hasIntervalMax ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty si:hasIntervalMin ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty si:hasStandardUnc ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty si:hasDistribution ;
                                      owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                    ] ;
                    skos:definition "Structure for stating a probabilistic-symmetric coverage interval for a real uncertainty, to be applied to an si:Real quantity value"@en ;
                    skos:editorialNote "changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel" ;
                    skos:prefLabel "coverage interval"@en ,
                                   "Überdeckungsintervall"@de .


###  https://ptb.de/si/ExpandedUnc
si:ExpandedUnc rdf:type owl:Class ;
               rdfs:subClassOf m4i:UncertaintyDeclaration ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty si:hasCoverageFactor ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty si:hasCoverageProbability ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty si:hasUncertainty ;
                                 owl:cardinality "1"^^xsd:nonNegativeInteger
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty si:hasDistribution ;
                                 owl:maxCardinality "1"^^xsd:nonNegativeInteger
                               ] ;
               skos:definition "Structure for stating an expanded measurement, model, or simulation uncertainty, e.g., to be applied to an si:Real quantity value"@en ;
               skos:editorialNote "changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel" ;
               skos:prefLabel "erweiterte Unsicherheit"@de ,
                              "expanded uncertainty"@en .


###  https://ptb.de/si/Real
si:Real rdf:type owl:Class ;
        rdfs:subClassOf pims-ii:QuantityValue ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty m4i:hasUncertaintyDeclaration ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty m4i:hasNumericalValue ;
                          owl:cardinality "1"^^xsd:nonNegativeInteger
                        ] ;
        skos:definition "(Real value +- uncertainty) x measurement unit, i.e., a scalar value with a measurement unit and an uncertainty indication"@en ;
        skos:editorialNote "changed by m4i: harmonized spelling to OBO naming conventions (lower case, space separated), added skos:prefLabel" ;
        skos:prefLabel "Real"@en ,
                       "Reell"@de .


###  https://schema.org/Intangible
schema:Intangible rdf:type owl:Class ;
                  skos:definition "A utility class that serves as the umbrella for a number of 'intangible' things such as quantities, structured values, etc."@en ;
                  skos:editorialNote "changed by m4i: added skos:prefLabel, added german label"@en ;
                  skos:prefLabel "Intangible"@en ,
                                 "Ungreifbares"@de .


